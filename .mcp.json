{
  "mcpServers": {
    "python-debugger": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "python-debugger-mcp"
      ],
      "env": {}
    },
    "playwright": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@playwright/mcp@latest"
      ]
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "grep": {
      "type": "http",
      "url": "https://mcp.grep.app"
    },
    "spec-workflow-mcp": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "spec-workflow-mcp@latest"
      ],
      "env": {}
    }
  }
}